    "es5-shim": "~4.1.0",
    "eslint": "^0.21.1",
    "eslint-plugin-react": "^2.3.0",
    "jasmine": "~2.2.1",
    "jasmine-core": "~2.2.0",
    "karma": "~0.12.31",
    "karma-jasmine": "~0.3.5",
    "karma-phantomjs-launcher": "~0.1.4",
    "karma-sourcemap-loader": "~0.3.4",
    "karma-webpack": "~1.5.0",
    "sass-loader": "~1.0.2",
